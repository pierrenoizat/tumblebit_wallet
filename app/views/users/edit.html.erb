<div class="container">
  <div class="row">
	<div class="authform">
  	<%= form_for(@user) do |f| %>
    <div class="form-group">
      <%= f.label :email %>
      <%= f.email_field :email, class: 'form-control' %>
    </div>
    <%= f.submit 'Update', class: "btn btn-primary" %>
  	<% end %>
  	</div>
  </div>
</div>